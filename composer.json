{
    "name": "supabase-php/supabase-client",
    "description": "Supabase client for PHP",
    "keywords": ["supabase","supabase-php","supabase-client"],
    "version": "1.0.6",
    "type": "library",
    "require": {
      "php": "^8.1"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^5.6",
        "guzzlehttp/guzzle": "^7.9",
        "pestphp/pest": "^3.7",
        "phpstan/phpstan": "^2.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Supabase\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "authors": [
        {
            "name": "Sushil Kumar",
            "email": "sushilkumbhar77@gmail.com",
            "role": "Developer"
        },
        {
          "name": "Ashish Kumbhar",
          "email": "ashishkumbhar01@hotmail.com",
          "role": "Developer"
        }
    ],
    "scripts": {
      "test": [
        "@php vendor/bin/phpstan analyse src tests --ansi"
      ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
